{
  "manifest_version": 2,
  "name" : "Picasso - A Google Calendar theme",
  "description" : "A prettier Google Calendar theme",
  "version" : "1.0.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [{
    "css": ["styles.css"],
    "matches": ["https://calendar.google.com/*"]
  }],
  "icons": {
   "128": "logo.png"
  }
}
