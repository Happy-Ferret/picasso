{
  "manifest_version": 2,
  "name" : "Prettier Google Calendar skin",
  "description" : "Skin that makes the Google Calendar prettier",
  "version" : "1.0.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [{
    "css": ["styles.css"],
    "matches": ["https://calendar.google.com/*"]
  }],
  "icons": {
   "128": "logo.png"
  }
}
